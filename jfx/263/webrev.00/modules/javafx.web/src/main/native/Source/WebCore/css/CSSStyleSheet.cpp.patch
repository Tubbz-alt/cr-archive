+#endif // ASSERT_ENABLED
 
 Ref<CSSStyleSheet> CSSStyleSheet::create(Ref<StyleSheetContents>&& sheet, CSSImportRule* ownerRule)
 {
     return adoptRef(*new CSSStyleSheet(WTFMove(sheet), ownerRule));
 }
